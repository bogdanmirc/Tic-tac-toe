<Application x:Class="TicTacToe.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TicTacToe"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor" Color="#f48a2e"></SolidColorBrush>
        <SolidColorBrush x:Key="ButtonColor" Color="#ffb966"></SolidColorBrush>
        <SolidColorBrush x:Key="LineColor" Color="#01fd0a"></SolidColorBrush>
    </Application.Resources>
</Application>
